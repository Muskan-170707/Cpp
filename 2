#include <iostream>
using namespace std;

class MyClass {
public:
    static int staticVar; 
};

int MyClass::staticVar = 5; 

int main() {
    cout << "Static variable from MyClass: " << MyClass::staticVar << endl;

    cout << "Muskan Y. Chandera" << endl;
    cout << "220130318043" << endl;

    return 0;
}
