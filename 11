#include <iostream>
using namespace std;

class ParameterizedConstructor {
private:
    int num;

public:
    ParameterizedConstructor(int value) {
        num = value;
    }

    void display() {
        cout << "Value: " << num << endl;
    }
};

int main() {
    ParameterizedConstructor pc(42);
    pc.display();

    cout << "Muskan Y. Chandera" << endl;
    cout << "220130318043" << endl;

    return 0;
}
