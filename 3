#include <iostream>
using namespace std;

class MyClass {
public:
    void display(); 
};

void MyClass::display() {
    cout << "Function defined outside the class using scope resolution operator." << endl;
}

int main() {
    MyClass obj;
    obj.display();

    cout << "Muskan Y. Chandera" << endl;
    cout << "220130318043" << endl;

    return 0;
}
