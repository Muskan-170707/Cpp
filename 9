#include <iostream>
using namespace std;

int main() {
    int num;

    cout << "Enter an integer: ";
    cin >> num;

    if (num % 2 == 0) {
        cout << num << " is an even integer." << endl;
    } else {
        cout << num << " is an odd integer." << endl;
    }

    cout << "Muskan Y. Chandera" << endl;
    cout << "220130318043" << endl;

    return 0;
}
