#include <iostream>
using namespace std;

enum Color { Red, Green, Blue };

int main() {
    Color selectedColor = Green;

    cout << "Selected color: ";
    switch (selectedColor) {
        case Red:
            cout << "Red";
            break;
        case Green:
            cout << "Green";
            break;
        case Blue:
            cout << "Blue";
            break;
    }
    cout << endl;

    cout << "Muskan Y. Chandera" << endl;
    cout << "220130318043" << endl;

    return 0;
}
