#include <iostream>
using namespace std;

inline int square(int num) {
    return num * num;
}

int main() {
    int number;

    cout << "Enter a number: ";
    cin >> number;

    int result = square(number);

    cout << "Square of " << number << " is " << result << endl;

    cout << "Muskan Y. Chandera" << endl;
    cout << "220130318043" << endl;

    return 0;
}
