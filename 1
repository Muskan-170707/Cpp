#include <iostream>
using namespace std;

class Employee {
private:
    int emp_id;
    string emp_name;

public:
    void getdata() {
        cout << "Enter Employee ID: ";
        cin >> emp_id;
        cout << "Enter Employee Name: ";
        cin.ignore();
        getline(cin, emp_name);
    }

    void putdata() {
        cout << "Employee ID: " << emp_id << endl;
        cout << "Employee Name: " << emp_name << endl;
    }
};

