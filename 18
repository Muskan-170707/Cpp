#include <iostream>
using namespace std;

double calculateArea(double radius, double pi = 3.14159265359) {
    return pi * radius * radius;
}

int main() {
    double r;
    cout << "Enter the radius of the circle: ";
    cin >> r;

    double area = calculateArea(r);

    cout << "Area of the circle: " << area << endl;

    cout << "Muskan Y. Chandera" << endl;
    cout << "220130318043" << endl;

    return 0;
}
